translate ko game_script_b5d5ba8a:

    # s "Thank you."
    s "고마워."

# game/game_script.rpy:513
translate ko game_script_7507c9be:

    # s "I was too scared to tell them myself."
    s "나 혼자선 말할 용기가 없었어."

# game/game_script.rpy:516
