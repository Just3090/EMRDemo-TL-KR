translate ko game_script_2815d089:

    # "As will I."
    "나도 마찬가지고."

# game/game_script.rpy:3568
translate ko game_script_a28869be_2:

    # n "But..."
    n "그래도..."

# game/game_script.rpy:3569
translate ko game_script_8ca87477:

    # n "I’m...{w}here."
    n "나...{w}여기 있어."

# game/game_script.rpy:3571
